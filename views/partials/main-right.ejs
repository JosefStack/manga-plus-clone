<div id="main-right">
    <div class="socials-wrapper">
        <div class="socials-container">
            <span>Official SNS Accounts</span>
            <div class="links-container">
                <a href="">
                    <img src="https://mangaplus.shueisha.co.jp/img/x_color_logo.2985e51e.svg" alt="">
                </a>

                <a href="">
                    <img src="data:image/png;base64," alt="">
                </a>

                <a href="">
                    <img src="https://mangaplus.shueisha.co.jp/img/youtube_logo.b7a7c53a.svg" alt="">
                </a>

                <a href="">
                    <img src="https://mangaplus.shueisha.co.jp/img/discord_color_logo.3f8e5b57.svg" alt="">
                </a>

                <a href="">
                    <img src="https://mangaplus.shueisha.co.jp/img/facebook_color_logo.7fd095ad.svg" alt="">
                </a>
            </div>
        </div>
    </div>


    <div class="leaderboard-container">
        <div class="leaderboard-header">
            <span>Hottest</span>
            <a href="">
                <span>View All</span>
                <i class='bx  bx-chevron-right'></i> 
            </a>
        </div>

        <div class="leaderboard-cards-wrapper">
            <% for(let i = 0; i < mangas.length; i++) { %>

                <div class="leaderboard-card-container">
                    <a href="">
                        <div class="leaderboard-card-img">
                            <img src="<%= mangas[i].img %>" alt="">
                        </div>
                        <span><%= mangas[i].rank %></span>
                        <div class="leaderboard-card-text">
                            <span><%= mangas[i].name %></span>
                            <span><%= mangas[i].author %></span>
                            <span><%= mangas[i].likes %></span>
                        </div>
                    </a>
                </div>

            <% } %>
        </div>
    </div>


</div>